{
    "name": "typescript-template",
    "version": "0.0.1",
    "license": "MIT",
    "bin": "./bin/start",
    "files": [
        "bin/",
        "dist/",
        "package.json",
        "README.md"
    ],
    "main": "dist/main.js",
    "types": "dist/main.d.ts",
    "scripts": {
        "test": "npm run test:clean && npm run test:build && npx mocha",
        "test:build": "npx tsc -p ./tests/tsconfig.json",
        "test:debug": "npm run test:clean && npm run test:build && NODE_OPTIONS=--inspect-brk npx mocha --timeout 9999999",
        "test:clean": "rm -rf ./dist-test",
        "build": "npx tsc -p ./tsconfig.json",
        "start": "node ./dist/main.js",
        "clean": "rm -rf ./dist",
        "pub": "npm run clean && npm run build && npm publish"
    },
    "dependencies": {
        "axios": "~1.1.3",
        "neverthrow": "^5.1.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/mocha": "^10.0.0",
        "@types/node": "^18.11.9",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "chai": "^4.3.7",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "^10.1.0",
        "typescript": "^4.9.3"
    }
}
